{
  "name": "vue3-zoomer",
  "version": "1.0.7",
  "description": "A Vue 3 component for zooming images.",
  "type": "module",
  "packageManager": "bun@1.1.33",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build && vue-tsc && node ./scripts/injectCss.js",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^1.6.4",
    "vue-tsc": "^3.2.4"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "vue",
    "zoom",
    "zoomer",
    "magnifier",
    "images",
    "composition",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue3-zoomer/vue3-zoomer.git"
  },
  "bugs": {
    "url": "https://github.com/vue3-zoomer/vue3-zoomer/issues"
  },
  "homepage": "https://vue3-zoomer.vuedoo.org",
  "module": "./dist/vue3-zoomer.es.js",
  "style": "./dist/style.css",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue3-zoomer.es.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  }
}
